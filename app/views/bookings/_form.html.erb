<%= form_for([@venue, @venue.bookings.new]) do |f| %>

<div class="row">
<div class="input-field">
  <i class="material-icons prefix">today</i>
  <label for="icon_prefix">Pick a draft date</label>
  <%= f.date_field(:draft_date, class: 'datepicker')%>
</div>
<%= f.hidden_field :venue_id, value: @venue.id %>
<%= f.hidden_field :user_id, value: current_user.id %>
<div class="actions center-align">
  <%= f.submit "Book Now", :class=>'waves-effect waves-blue waves-ripple btn blue-grey darken-2' %>
</div>
<% end %>
</div>
